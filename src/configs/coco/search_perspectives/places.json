{
    "id": "places",
    "endpoint": {
        "url": "https://ldf.fi/coco-dev/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesPlaces.js",
    "baseURI": "http://ldf.fi/coco",
    "URITemplate": "<BASE_URI>/places/<LOCAL_ID>",
    "facetClass": "crm:E53_Place",
    "defaultConstraint": "FILTER EXISTS { <SUBJECT> :number_of_events [] }",
    "frontPageImage": "main_page/places2-452x262.jpg",
    "searchMode": "faceted-search",
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "places": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "placePropertiesFacetResults",
                "pagesize": 30,
                "paginatedResultsAlwaysExpandRows": true,
                "paginatedResultsRowContentMaxHeight": 150,
                "sortBy": "number_of_events",
                "sortDirection": "desc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "placePropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "placeInstancePageLetters": {
                        "tabID": 1,
                        "component": "InstancePageTable",
                        "tabPath": "letters-and-actors",
                        "tabIcon": "MailOutline",
                        "fetchResultsWhenMounted": true,
                        "sparqlQuery": "placeLettersInstancePageQuery",
                        "properties": [
                            {
                                "id": "from",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            },
                            {
                                "id": "mentioningletter",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            },
                            {
                                "id": "related",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            },
                            {
                                "id": "born",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": true,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            },
                            {
                                "id": "deceased",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": true,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            }
                        ]
                    },
                    "lettersByYear": {
                        "tabID": 2,
                        "component": "ApexCharts",
                        "tabPath": "letters-by-year",
                        "tabIcon": "ShowChart",
                        "sparqlQuery": "sentReceivedByPlaceQuery",
                        "facetClass": "places",
                        "filterTarget": "instance",
                        "resultMapper": "mapLineChart",
                        "resultMapperConfig": {
                            "fillEmptyValues": false
                        },
                        "createChartData": "createSingleLineChartData",
                        "title": "Letters by year",
                        "xaxisTitle": "Year",
                        "xaxisType": "category",
                        "xaxisTickAmount": 20,
                        "yaxisTitle": "Number of letters",
                        "seriesTitle": "Number of letters",
                        "stroke": {
                            "curve": "straight",
                            "width": 2
                        }
                    },
                    "instancePageExport": {
                        "tabID": 3,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "export": {
            "tabID": 1,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "CloudDownload",
            "resultClass": "perspective1",
            "facetClass": "perspective1"
        }
    },
    "properties": [
        {
            "id": "image",
            "valueType": "image",
            "previewImageHeight": 160,
            "makeLink": false,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "hideHeader": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false
        },
        {
            "id": "altLabel",
            "valueType": "string",
            "renderAsHTML": false,
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "location",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "country",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "broader",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "number_of_events",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false
        },
        {
            "id": "narrower",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "external",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        }
    ],
    "facets": {
        "prefLabel": {
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "sortByPredicate": "skos:prefLabel",
            "textQueryProperty": "skos:prefLabel"
        },
        "country": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": ":country",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "searchField": true,
            "sortButton": true,
            "sortByPredicate": ":country/skos:prefLabel"
        },
        "broader": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "skos:broader",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "searchField": true,
            "sortButton": true,
            "sortByPredicate": "skos:broader/skos:prefLabel"
        },
        "number_of_events": {
            "sortByPattern": "{ SELECT ?id ?orderBy WHERE { VALUES ?facetClass { <FACET_CLASS> } ?id a ?facetClass . OPTIONAL { ?id :number_of_events ?num } BIND (COALESCE(?num, 0) AS ?orderBy) }}"
        }
    }
}